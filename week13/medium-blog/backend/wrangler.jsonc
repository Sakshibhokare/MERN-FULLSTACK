  "vars": {
    "DATABASE_URL": "prisma+postgres://accelerate.prisma-data.net/?api_key=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..puWIsI5RztodZ_J1aJin-haAPQAkgp5oeeKv9FviAg0",
    "JWT_SECRET":"secret"
  },
  "kv_namespaces": [
    {
